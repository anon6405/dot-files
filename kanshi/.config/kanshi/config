profile multimon {
    output HDMI-A-1 enable position 0,0 scale 0.8
    output eDP-1 enable position 0,1080 scale 1
    exec swaymsg workspace 3, move workspace to HDMI-A-1
}

profile singlemon{
    output HDMI-A-1 enable mode 1366x768 scale 0.9
    output eDP-1 disable
}

profile {
    output eDP-1 enable scale 1
}
