! special
*.foreground:   #2f2f2f
*.background:   #f3f4f5
*.cursorColor:  #565e65

! black
*.color0:       #1c2023
*.color8:       #747c84

! red
*.color1:       #ce3c1e
*.color9:       #ce3c1e

! green
*.color2:       #1ea85c
*.color10:      #1ea85c

! yellow
*.color3:       #758268
*.color11:      #758268

! blue
*.color4:       #50719b
*.color12:      #50719b

! magenta
*.color5:       #aa297f
*.color13:      #aa297f

! cyan
*.color6:       #35a390
*.color14:      #35a390

! white
*.color7:       #c7ccd1
*.color15:      #f3f4f5

URxvt.cursorBlink : 1
URxvt.font       :xft:JetBrainsMono Nerd Font Mono:pixelsize=12.5:style=Regular \
                  xft:DejaVu Sans Mono:size=12.5:style=Medium

URxvt.italicFont :xft:JetBrainsMono Nerd Font Mono:pixelsize=12.5:style=Italic
URxvt.boldFont   :xft:JetBrainsMono Nerd Font Mono:pixelsize=12.5:style=Bold

URxvt.lineSpace           : 0
URxvt.letterSpace         : 0
URxvt.scrollBar           : false
URxvt.internalBorder      : 25
URxvt.cursorUnderline     : false
URxvt.geometry            : 70x35
URxvt.thickness           : 5
URxvt.titleBar            : false
URxvt.intensityStyles     : false


URxvt.perl-ext-common : default,matcher,font-size
URxvt.font-size.step: 1
URxvt.url-launcher    : chromium
URxvt.matcher.button  : 1

Xft.autohint: true
Xft.antialias: true
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
Xft.dpi: 96
