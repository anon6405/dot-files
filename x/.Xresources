! special
*.foreground:   #2f2f2f
*.background:   #f3f4f5
*.cursorColor:  #565e65

! black
*.color0:       #1c2023
*.color8:       #747c84

! red
*.color1:       #b55651
*.color9:       #b55651

! green
*.color2:       #11703c
*.color10:      #11703c

! yellow
*.color3:       #4d4f4b
*.color11:      #4d4f4b

! blue
*.color4:       #304560
*.color12:      #304560

! magenta
*.color5:       #aa297f
*.color13:      #aa297f

! cyan
*.color6:       #4ba39a
*.color14:      #4ba39a

! white
*.color7:       #c7ccd1
*.color15:      #f3f4f5

URxvt.cursorBlink : 1
URxvt.font       :xft:JetBrainsMono Nerd Font Mono:pixelsize=13:style=Regular \
                  xft:DejaVu Sans Mono:size=13:style=Medium

URxvt.italicFont :xft:JetBrainsMono Nerd Font Mono:pixelsize=13:style=Italic
URxvt.boldFont   :xft:JetBrainsMono Nerd Font Mono:pixelsize=13:style=Bold

URxvt.lineSpace           : 0
URxvt.letterSpace         : 0
URxvt.scrollBar           : false
URxvt.internalBorder      : 25
URxvt.cursorUnderline     : false
URxvt.geometry            : 80x30
URxvt.thickness           : 5
URxvt.titleBar            : false
URxvt.intensityStyles     : false


URxvt.perl-ext-common : default,matcher,font-size
URxvt.font-size.step: 1
URxvt.url-launcher    : chromium
URxvt.matcher.button  : 1

Xft.antialias:	1
Xft.hinting:	1
Xft.autohint:	0
Xft.hintstyle:	hintslight
Xft.rgba:	rgb
Xft.lcdfilter:	lcddefault
